<?xml version="1.0" ?><xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file source-language="en" datatype="plaintext" original="src/Resources/translations/tl_metamodel_dcasetting.en.xlf" target-language="de">
    <body>
      <trans-unit id="rgxp.label" resname="rgxp.label">
        <source>Regular expression</source>
        <target>Regular Expression</target>
      </trans-unit>
      <trans-unit id="rgxp.description" resname="rgxp.description">
        <source>Validates the field against a regular expression.</source>
        <target>Validierung des Feldes mit Regular Expression</target>
      </trans-unit>
      <trans-unit id="rgxp_options.digit" resname="rgxp_options.digit">
        <source>Numeric characters</source>
        <target>Numerische Zeichen</target>
      </trans-unit>
      <trans-unit id="rgxp_description.digit" resname="rgxp_description.digit">
        <source>Allows numeric characters only (including full stop [.] and minus [-])</source>
        <target>erlaubt nur numerische Zeichen (einschließlich Punkt [.] und Minus [-])</target>
      </trans-unit>
      <trans-unit id="rgxp_options.natural" resname="rgxp_options.natural">
        <source>Non-negative natural numbers</source>
        <target>Nichtnegative natürliche Zahlen</target>
      </trans-unit>
      <trans-unit id="rgxp_description.natural" resname="rgxp_description.natural">
        <source>Allows non-negative natural numbers (including 0)</source>
        <target>erlaubt nicht-negative natürliche Zahlen (einschließlich 0)</target>
      </trans-unit>
      <trans-unit id="rgxp_options.alpha" resname="rgxp_options.alpha">
        <source>Alphabetic characters</source>
        <target>Alphabetische Zeichen</target>
      </trans-unit>
      <trans-unit id="rgxp_description.alpha" resname="rgxp_description.alpha">
        <source>Allows alphabetic characters only (including full stop [.] minus [-] and space [ ])</source>
        <target>erlaubt nur alphabetische Zeichen (einschließlich Punkt [.], Minus [-] und Leerzeichen [ ])</target>
      </trans-unit>
      <trans-unit id="rgxp_options.alnum" resname="rgxp_options.alnum">
        <source>Alphanumeric characters</source>
        <target>Alphanumerische Zeichen</target>
      </trans-unit>
      <trans-unit id="rgxp_description.alnum" resname="rgxp_description.alnum">
        <source>Allows alphanumeric characters only (including full stop [.] minus [-], underscore [_] and space [ ])</source>
        <target>erlaubt nur alphanumerische Zeichen (einschließlich Punkt [.], Minus [-], Unterstrich [_] und Leerzeichen [ ])</target>
      </trans-unit>
      <trans-unit id="rgxp_options.extnd" resname="rgxp_options.extnd">
        <source>Extend special characters</source>
        <target>Sonderzeichen</target>
      </trans-unit>
      <trans-unit id="rgxp_description.extnd" resname="rgxp_description.extnd">
        <source><![CDATA[Disallows #<>()\\=]]></source>
        <target>verbietet #&lt;&gt;()\\=</target>
      </trans-unit>
      <trans-unit id="rgxp_options.date" resname="rgxp_options.date">
        <source>Date</source>
        <target>Datum</target>
      </trans-unit>
      <trans-unit id="rgxp_description.date" resname="rgxp_description.date">
        <source>Expects a valid date</source>
        <target>erwartet ein gültiges Datum</target>
      </trans-unit>
      <trans-unit id="rgxp_options.time" resname="rgxp_options.time">
        <source>Time</source>
        <target>Zeit</target>
      </trans-unit>
      <trans-unit id="rgxp_description.time" resname="rgxp_description.time">
        <source>Expects a valid time</source>
        <target>erwartet eine gültige Uhrzeit</target>
      </trans-unit>
      <trans-unit id="rgxp_options.datim" resname="rgxp_options.datim">
        <source>Date and time</source>
        <target>Datum und Uhrzeit</target>
      </trans-unit>
      <trans-unit id="rgxp_description.datim" resname="rgxp_description.datim">
        <source>Expects a valid date and time</source>
        <target>erwartet ein gültiges Datum und eine gültige Uhrzeit</target>
      </trans-unit>
      <trans-unit id="rgxp_options.friendly" resname="rgxp_options.friendly">
        <source>Friendly email</source>
        <target>E-Mail im &quot;Friendly Name Format&quot;</target>
      </trans-unit>
      <trans-unit id="rgxp_description.friendly" resname="rgxp_description.friendly">
        <source>Expects a valid &quot;friendly name format&quot; e-mail address</source>
        <target>erwartet eine gültige E-Mail-Adresse im &quot;Friendly Name Format&quot;.</target>
      </trans-unit>
      <trans-unit id="rgxp_options.email" resname="rgxp_options.email">
        <source>Email</source>
        <target>E-Mail</target>
      </trans-unit>
      <trans-unit id="rgxp_description.email" resname="rgxp_description.email">
        <source>Expects a valid e-mail address</source>
        <target>erwartet eine gültige E-Mail Adresse</target>
      </trans-unit>
      <trans-unit id="rgxp_options.emails" resname="rgxp_options.emails">
        <source>Emails</source>
        <target>E-Mails</target>
      </trans-unit>
      <trans-unit id="rgxp_description.emails" resname="rgxp_description.emails">
        <source>Expects a valid list of valid e-mail addresses</source>
        <target>erwartet eine Liste mit gültigen E-Mail-Adressen</target>
      </trans-unit>
      <trans-unit id="rgxp_options.url" resname="rgxp_options.url">
        <source>URL</source>
        <target>URL</target>
      </trans-unit>
      <trans-unit id="rgxp_description.url" resname="rgxp_description.url">
        <source>Expects a valid URL</source>
        <target>erwartet eine gültige URL</target>
      </trans-unit>
      <trans-unit id="rgxp_options.alias" resname="rgxp_options.alias">
        <source>Alias</source>
        <target>Alias</target>
      </trans-unit>
      <trans-unit id="rgxp_description.alias" resname="rgxp_description.alias">
        <source>Expects a valid alias</source>
        <target>erwartet einen gültigen Alias</target>
      </trans-unit>
      <trans-unit id="rgxp_options.folderalias" resname="rgxp_options.folderalias">
        <source>Folder alias</source>
        <target>Ordner-Alias</target>
      </trans-unit>
      <trans-unit id="rgxp_description.folderalias" resname="rgxp_description.folderalias">
        <source>Expects a valid folder URL alias</source>
        <target>erwartet einen gültigen Alias der Ordner-URL</target>
      </trans-unit>
      <trans-unit id="rgxp_options.phone" resname="rgxp_options.phone">
        <source>Phone</source>
        <target>Telefonnummer</target>
      </trans-unit>
      <trans-unit id="rgxp_description.phone" resname="rgxp_description.phone">
        <source>Expects a valid phone number (numeric characters, space [ ], plus [+], minus [-], parentheses [()] and slash [/])</source>
        <target>erwartet eine gültige Telefonnummer (numerische Zeichen, Leerzeichen [ ], Plus [+], Minus [-], Klammern [()] und Schrägstrich [/]</target>
      </trans-unit>
      <trans-unit id="rgxp_options.prcnt" resname="rgxp_options.prcnt">
        <source>Percent value</source>
        <target>Prozent-Wert</target>
      </trans-unit>
      <trans-unit id="rgxp_description.prcnt" resname="rgxp_description.prcnt">
        <source>Allows numbers between 0 and 100</source>
        <target>erlaubt Zahlen zwischen 0 und 100</target>
      </trans-unit>
      <trans-unit id="rgxp_options.locale" resname="rgxp_options.locale">
        <source>Locale key</source>
        <target>Locale-Angabe</target>
      </trans-unit>
      <trans-unit id="rgxp_description.locale" resname="rgxp_description.locale">
        <source>Expects a valid locale (e.g. &quot;de-CH&quot;)</source>
        <target>erwartet eine gültige Locale-Angabe (z. B. &quot;de-CH&quot;)</target>
      </trans-unit>
      <trans-unit id="rgxp_options.language" resname="rgxp_options.language">
        <source>Language key</source>
        <target>Sprachen-Code</target>
      </trans-unit>
      <trans-unit id="rgxp_description.language" resname="rgxp_description.language">
        <source>Expects a valid language code (e.g. &quot;EN&quot;)</source>
        <target>erwartet einen gültigen Sprachen-Code (z. B. &quot;EN&quot;)</target>
      </trans-unit>
      <trans-unit id="rgxp_options.fieldname" resname="rgxp_options.fieldname">
        <source>Fieldname</source>
        <target>Feldname</target>
      </trans-unit>
      <trans-unit id="rgxp_description.fieldname" resname="rgxp_description.fieldname">
        <source>Expects a valid field name</source>
        <target>erwartet einen gültigen Feldnamen</target>
      </trans-unit>
    </body>
  </file>
